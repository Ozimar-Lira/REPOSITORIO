@model Loteamento;

@{
    ViewData["Title"] = "Cadastro Loteamento";

}

<h6>Cadastramento de Lotes</h6>

<p></p>



<form method="POST" asp-action="Cadastro">
    <div class="form-group">
      
        <p>
            <label asp-for="Nome_Lote" class="label_normal">Nome Lote:</label>
            <input asp-for="Nome_Lote" class="form-control" size="24">
        </p>
        <p>
            <label asp-for="Nome_Cliente" class="label_normal">Nome Cliente:</label>
            <input asp-for="Nome_Cliente" class="form-control" size="24">
        </p>

        <p>
            <label asp-for="CPF" class="label_normal">CPF:</label>
            <input asp-for="CPF" class="form-control" size="24">
        </p>
        <p>
            <label asp-for="Valor_Lote" class="label_normal">Valor Lote:</label>
            <input asp-for="Valor_Lote" class="form-control" size="24">
        </p>
        <p>
            <label asp-for="Entrada" class="label_normal">Entrada:</label>
            <input asp-for="Entrada" class="form-control" size="24">
        </p>

        <p>
            <label asp-for="Qtd_Parcelas" class="label_normal">Qtd Parcelas:</label>
            <input asp-for="Qtd_Parcelas" class="form-control" size="24">
        </p>

        <p>
            <label asp-for="Valor_Parcela" class="label_normal">Valor Parcela:</label>
            <input asp-for="Valor_Parcela" class="form-control" size="24">
        </p>

        <p>
            <label asp-for="Data_Contrato" class="label_normal">Data do Contrato:</label>
            <input asp-for="Data_Contrato" class="form-control" size="24">
        </p>

        <p>
            <input type="Submit" value="Cadastrar">
        </p>
    </div>
</form>
